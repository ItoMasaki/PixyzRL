<?xml version="1.0" encoding="UTF-8" ?>
<robot name="prototype1">

  <!-- ******************
       base_link
       ****************** -->
  <link name="base_link">
    <!-- 慣性要素（例） -->
    <inertial>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <mass value="1.0"/>
      <inertia ixx="0.01" ixy="0.0" ixz="0.0" iyy="0.01" iyz="0.0" izz="0.01"/>
    </inertial>

    <!-- 可視形状 -->
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <box size="0.3 0.3 0.2"/>
      </geometry>
      <material name="body">
        <color rgba="1 1 1 0.5"/>
      </material>
    </visual>

    <!-- 衝突形状 -->
    <collision>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <box size="0.3 0.3 0.2"/>
      </geometry>
    </collision>
  </link>

  <!-- ******************
       base_left_hip_joint
       ****************** -->
  <joint name="base_left_hip_joint" type="revolute">
    <origin xyz="-0.15 -0.05 -0.05" rpy="1.57 0 0"/>
    <parent link="base_link"/>
    <child link="left_hip_link"/>
    <axis xyz="0 0 1"/>
    <!-- limit に effort / velocity を追加 -->
    <limit lower="-0.2" upper="0.2" effort="10" velocity="1.0"/>
  </joint>

  <!-- ******************
       left_hip_link
       ****************** -->
  <link name="left_hip_link">
    <inertial>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <mass value="0.3"/>
      <inertia ixx="0.003" ixy="0.0" ixz="0.0" iyy="0.003" iyz="0.0" izz="0.003"/>
    </inertial>
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <cylinder radius="0.1" length="0.1"/>
      </geometry>
      <material name="body">
        <color rgba="1 1 1 0.5"/>
      </material>
    </visual>
    <collision>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <cylinder radius="0.1" length="0.1"/>
      </geometry>
    </collision>
  </link>

  <!-- ******************
       base_right_hip_joint
       ****************** -->
  <joint name="base_right_hip_joint" type="revolute">
    <origin xyz="0.15 -0.05 -0.05" rpy="1.57 0 0"/>
